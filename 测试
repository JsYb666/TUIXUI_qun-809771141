Part = Instance.new("Part", game.Workspace)
Part.Anchored = true
Part.CanCollide = false
Part.CastShadow = false
Part.Material = "ForceField"
Part.Shape = "Ball"

Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/nahida-cn/Roblox/main/UI.lua"))()

Window = Library:Window("æ—", "åˆ€åˆƒçƒ")

Player = Window:Tab("ç©å®¶")

Player:Slider("ç§»åŠ¨é€Ÿåº¦", 0, 500, game.Players.LocalPlayer.Character.Humanoid.WalkSpeed, function(Value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
end)

Player:Slider("é‡åŠ›", 0, 500, math.round(game.Workspace.Gravity), function(Value)
    game.Workspace.Gravity = Value
end)

Player:Button("é£è¡Œ", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/nahida-cn/Roblox/main/Fly.lua"))()
end)

Player:Toggle("è¸ç©º", false, function(Value)
    Jump = Value
end)

Player:Toggle("ç©¿å¢™", false, function(Value)
    Noclip = Value
    if not Noclip then
        game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")
    end
end)

Loop = Window:Tab("å¾ªç¯")

Loop:Textbox("ç”¨æˆ·å", true, function(Value)
    User = Value
end)

Loop:Toggle("ä¼ é€", false, function(Value)
    LT = Value
    game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled("Seated", not LT)
end)

Auto = Window:Tab("è‡ªåŠ¨")

Auto:Toggle("å‡»é€€", false, function(Value)
    Parry = Value
end)

Auto:Toggle("ä¼ é€", false, function(Value)
    Teleport = Value
end)

ESP = Window:Tab("é€è§†")

ESP:Toggle("èŒƒå›´", false, function(Value)
    Aura = Value
end)

Other = Window:Tab("å…¶ä»–")

Other:Button("å»ºç­‘å·¥å…·", function()
    for i = 3, 4 do
        HB = Instance.new("HopperBin", game.Players.LocalPlayer.Backpack)
        HB.BinType = i
    end
end)

Other:Button("ç‚¹å‡»ä¼ é€", function()
    Tool = Instance.new("Tool", game.Players.LocalPlayer.Backpack)
    Tool.RequiresHandle = false
    Tool.Activated:Connect(function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer:GetMouse().Hit+Vector3.new(0, 2.5, 0)
    end)
end)

About = Window:Tab("ğŸ˜±ğŸ˜¨ğŸ˜°")

About:Label("ä½œè€…:æ—å¤©å¸")

About:Button("å¤åˆ¶ç¾¤èŠ", function()
    setclipboard("")
end)

game.RunService.Stepped:Connect(function()
    if Noclip then
        for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if v:IsA("BasePart") then
                v.CanCollide = false
            end
        end
    end
end)

game.RunService.Stepped:Connect(function()
    if LT then
        for i, v in pairs(game.Players:GetPlayers()) do
            if string.find(v.Name, User) then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
            end
        end
    end
end)

game.RunService.Stepped:Connect(function()
    if #game.Workspace.Balls:GetChildren() == 0 or not Aura then
        Part.Size = Vector3.zero
    end
    Part.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
end)

game.RunService.Stepped:Connect(function()
    for i, v in pairs(game.Workspace.Balls:GetChildren()) do
        if v:GetAttribute("realBall") then
            Velocity = (v.zoomies.VectorVelocity-game.Players.LocalPlayer.Character.Humanoid:GetMoveVelocity()).Magnitude/2
            if Parry and game.Players.LocalPlayer.Character:FindFirstChild("Highlight") then
                if Teleport and Velocity ~= 0 then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                end
                if (v.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < Velocity then
                    game.ReplicatedStorage.Remotes.ParryButtonPress:Fire()
                end
            end
            if Aura then
                if Velocity > 30 then
                    Velocity = 30
                end
                Part.Size = Vector3.new(Velocity, Velocity, Velocity)
                if game.Players.LocalPlayer.Character:FindFirstChild("Highlight") then
                    Part.Color = Color3.new(1, 0, 0)
                else
                    Part.Color = Color3.new(0, 1, 0)
                end
            end
        end
    end
end)

game.UserInputService.JumpRequest:Connect(function()
    if Jump then
        game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")
    end
end)
